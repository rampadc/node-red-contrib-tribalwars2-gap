{
  "name": "node-red-contrib-tribalwars2-gap",
  "version": "1.0.0",
  "description": "A visual tool for wiring commands for Tribal Wars 2, runs on Node-RED. Short-named tw2-gap, it is a collection of nodes built specifically for the purpose of organizing attack and support commands for the game.",
  "main": "fn_plan.js",
  "directories": {
    "example": "examples"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rampadc/node-red-contrib-tribalwars2-gap.git"
  },
  "keywords": [
    "tw2",
    "node-red"
  ],
  "node-red": {
    "nodes": {
      "fn_plan": "fn_plan.js",
      "in_attackModifier": "in_attackModifier.js",
      "in_commandType": "in_commandType.js",
      "in_landingTime": "in_landingTime.js",
      "in_supportModifier": "in_supportModifier.js",
      "in_unit": "in_unit.js",
      "in_village": "in_village.js",
      "out_launchDateLocal": "out_launchDateLocal.js",
      "out_launchDateUTC": "out_launchDateUTC.js"
    }
  },
  "author": "Cong",
